{
  "name": "webui-infrastruture",
  "version": "0.0.1",
  "description": "components , services and every things about web application",
  "author": "morteza mousavi  <mousavi.mt5@gmail.com>",
  "homepage": "morteza.mousvi.me",
  "scripts": {
    "lint:lib": "ng lint main",
    "build:lib": "npm run lint:lib && npm run clean:dist && ng build main",
    "build": "npm run build:lib",
    "clean:dist": "rimraf dist",
    "copy:files": "cp ./README.md ./dist && cp ./LICENSE ./dist && cp -r ./projects/main/src/assets ./dist",
    "copy:files:win": "copy README.md .\\dist && copy LICENSE .\\dist && xcopy .\\projects\\main\\src\\assets .\\dist\\assets  /E /H /C /I",
    "doc": "typedoc --module commonjs --out doc lib/defs/api.ts",
    "clean:all": "npm run clean:dist && rimraf node_modules && npm cache clean --force",
    "clean:all:install": "npm run clean:all && npm i",
    "lint": "tslint lib/**/*.ts",
    "prepublish:lib:win": "npm run build:lib && npm run copy:files:win",
    "prepublish:lib": "npm run build:lib && npm run copy:files",
    "start:app": "ng serve --port 4600",
    "publish:release": "npm publish ./dist",
    "publish:next": "npm publish ./dist --tag next",
    "pack":"cd dist && npm pack",
    "publish:win":"npm run prepublish:lib:win && npm run pack",
    "publish":"npm run prepublish:lib && npm run pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "~13.3.9",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@ngx-translate/core": "^13.0.0",
    "ngx-cookie-service": "^11.0.2",
    "crypto-js": "^4.1.1",
    "moment-jalaali": "^0.9.6",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.0.4",
    "@angular/compiler-cli": "^14.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~4.0.0",
    "@types/moment-jalaali": "^0.7.6",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "typescript": "~4.7.2"
  }
}
